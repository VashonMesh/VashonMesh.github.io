---
import NormalLayout from '../../layouts/NormalLayout.astro';

export function getStaticPaths() {
  return [
    { params: { dog: "clifford" }},
    { params: { dog: "rover" }},
    { params: { dog: "spot" }},
  ];
}

const { dog } = Astro.params;
---
<NormalLayout title={`Good dog, ${dog}!`}>
  <div>Good dog, {dog}!</div>
</NormalLayout>